        "OCCMInstance": {
            "DependsOn": [
                "OCCMSecurityGroup",
                "OCCMInstanceProfile"
            ],
            "Type": "AWS::EC2::Instance",
            "Metadata": {
                "AWS::CloudFormation::Init": {
                    "config": {
                        "files": {
                            "/tmp/occm-params.txt": {
                                "content": {
                                    "Fn::Join": [
                                        "",
                                        [
                                            "name|",
                                            {
                                                "Ref": "AWS::StackName"
                                            },
                                            "\n",
                                            "customer|",
                                            {
                                                "Ref": "CompanyName"
                                            },
                                            "\n",
                                            "vpcID|",
                                            {
                                                "Ref": "VPCId"
                                            },
                                            "\n",
                                            "subnetID1|",
                                            {
                                                "Ref": "SubnetId"
                                            },
                                            "\n",
                                            "subnetID2|",
                                            {
                                                "Ref": "SubnetId2"
                                            },
                                            "\n",
                                            "subnetID3|",
                                            {
                                                "Ref": "SubnetId3"
                                            },
                                            "\n",
                                            "routeTableID|",
                                            {
                                                "Ref": "RouteTableID"
                                            },
                                            "\n",
                                            "floatIP1|",
                                            {
                                                "Ref": "FloatingIPCluster"
                                            },
                                            "\n",
                                            "floatIP2|",
                                            {
                                                "Ref": "FloatingIPData1"
                                            },
                                            "\n",
